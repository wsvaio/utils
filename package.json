{
  "name": "@wsvaio/utils",
  "type": "module",
  "version": "1.0.11",
  "description": "常用utils",
  "author": "wsvaio <wsvaio@qq.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/wsvaio",
  "homepage": "https://github.com/wsvaio/utils#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wsvaio/utils.git"
  },
  "bugs": {
    "url": "https://github.com/wsvaio/utils/issues"
  },
  "keywords": [
    "wsvaio",
    "utils"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./*": "./*"
  },
  "main": "index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=14"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "vitest",
    "commit": "czg",
    "build": "esno ./scripts/index.ts && tsup",
    "prepublishOnly": "npm run build",
    "log": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "lint:eslint": "eslint --cache --max-warnings 0  \"{src,mock}/**/*.{vue,ts,tsx}\" --fix",
    "lint:prettier": "prettier --write  \"src/**/*.{js,json,tsx,css,less,scss,vue,html,md}\"",
    "lint:lint-staged": "lint-staged",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.5",
    "@types/node": "^18.16.16",
    "@wsvaio/commitlint-config": "^0.0.4",
    "@wsvaio/eslint-config": "^0.0.4",
    "czg": "^1.6.1",
    "eslint": "^8.41.0",
    "esno": "^0.16.3",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "tsup": "^6.7.0",
    "typescript": "^4.9.5",
    "vitest": "^0.29.8"
  }
}
