{
  "name": "@wsvaio/utils",
  "type": "module",
  "version": "1.0.16",
  "description": "一个JavaScript工具库，它包含了许多通用的函数和工具，可以帮助您更快地编写JavaScript代码。",
  "author": "wsvaio <wsvaio@qq.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/wsvaio",
  "homepage": "https://github.com/wsvaio/utils#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wsvaio/utils.git"
  },
  "bugs": {
    "url": "https://github.com/wsvaio/utils/issues"
  },
  "keywords": [
    "wsvaio",
    "utils"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./*": "./*"
  },
  "main": "index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=14"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "vitest",
    "commit": "pnpm docs:build && git add docs && czg",
    "docs:build": "pnpm typedoc",
    "build": "esno ./scripts/index.ts && tsup",
    "prepublishOnly": "pnpm build",
    "log": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "lint:eslint": "eslint --cache --max-warnings 0  \"{src,mock}/**/*.{vue,ts,tsx}\" --fix",
    "lint:prettier": "prettier --write  \"src/**/*.{js,json,tsx,css,less,scss,vue,html,md}\"",
    "lint:lint-staged": "lint-staged",
    "prepare": "simple-git-hooks"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.5",
    "@types/node": "^20.3.1",
    "@wsvaio/commitlint-config": "^0.0.6",
    "@wsvaio/eslint-config": "^0.0.6",
    "czg": "^1.6.1",
    "eslint": "^8.42.0",
    "esno": "^0.16.3",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "simple-git-hooks": "^2.8.1",
    "tsup": "^6.7.0",
    "typedoc": "^0.24.8",
    "typescript": "^5.1.3",
    "vitest": "^0.32.0"
  }
}
